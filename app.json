{
  "expo": {
    "name": "RhythMark",
    "slug": "rhythmark",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundModes": [
        "background-processing",
        "background-fetch"
      ]
    },
    "android": {
      "permissions": [
        "WAKE_LOCK",
        "FOREGROUND_SERVICE"
      ]
    },
    "plugins": [
      "expo-notifications",
      "expo-task-manager",
      "expo-background-fetch"
    ]
  }
}
